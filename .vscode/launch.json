{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "SDRA: parse folder",
        "type": "debugpy",
        "request": "launch",
        "module": "myagents.simplified_sdra",
        "args": ["--parse", "D:/projects/simplified-sdra/requirements"],
        "cwd": "${workspaceFolder}",
        "console": "integratedTerminal",
        "justMyCode": true,
        "envFile": "${workspaceFolder}/.env",
        "python": "${workspaceFolder}/.venv/Scripts/python.exe"
      },
      {
        "name": "SDRA: joke smoke-test",
        "type": "debugpy",
        "request": "launch",
        "module": "myagents.simplified_sdra",
        "cwd": "${workspaceFolder}",
        "console": "integratedTerminal",
        "justMyCode": true,
        "envFile": "${workspaceFolder}/.env",
        "python": "${workspaceFolder}/.venv/Scripts/python.exe"
      },
      {
        "name": "Attach to running (debugpy)",
        "type": "debugpy",
        "request": "attach",
        "connect": { "host": "localhost", "port": 5678 }
      }
    ]
  }
  